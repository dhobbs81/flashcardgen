{
    "name": "flashcardgen",
    "version": "1.0.0-dev",
    "description": "A node based backend for a basic flashcard application",
    "keywords": [
        "bootcamp",
        "cli",
        "node",
        "javascript"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/dhobbs81/flashcardgen"
    },
    "scripts": {
        "build": "babel ./src -d ./lib -w",
        "start": "babel-node ./src/app.js --presets es2015,stage-2",
        "docs": "node_modules/.bin/jsdoc src/**.js --destination ./docs --private"
    },
    "dependencies": {
        "babel-cli": "^6.24.1",
        "chalk": "^1.1.3",
        "commander": "^2.9.0",
        "debug": "^2.6.8",
        "jsdoc": "^3.4.3"
    },
    "devDependencies": {
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1"
    },
    "contributors": [
        {
            "url": "https://github.com/dhobbs81/flashcardgen/graphs/contributors"
        }
    ],
    "maintainers": {
        "name": "Marcus Hobbs",
        "email": "dhobbs81@gmail.com"
    }
}
